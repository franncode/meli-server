{
	"version": 2,
	"builds": [{ "src": "index.js", "use": "@now/node-server" }],
	"routes": [
		{
			"src": "/api/trends",
			"dest": "/index.js",
			"methods": ["GET"]
		},
		{
			"src": "/api/items",
			"dest": "/index.js",
			"methods": ["GET"]
		}
		{
			"src": "/api/items/:id'",
			"dest": "/index.js",
			"methods": ["GET"]
		}
	]
}
